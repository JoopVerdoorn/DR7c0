  <strings>
    <string id="lClockFieldMetric">Metric in upper clock/lap field</string>
    <string id="lETAfromLap">Calculation of ETA based on elapsed time of last lap, instead of total average speed</string>

    <string id="lTEffect">Aerobic training effect</string>
    <string id="lETA">Estimated time of arrival</string>
    <string id="lETAfrompace">Estimated finished time based on average or last lap pace</string>
	<string id="lETAdeviation">Deviation from required finish in hours, minutes and seconds</string>
	<string id="lETArequiredpace">Required pace to meet required finish time</string>
    <string id="lElevationGain">Elevation gain</string>
    <string id="lElevationLoss">Elevation loss</string>
    <string id="lHeartrateZone">Heartrate zone</string>

    <string id="lMaxColoringPace">Use 5 colors for coloring of pace field in middle row, instead of 3 colors</string>
    <string id="lColoringPaceFromAver">Use average pace for coloring of the pace field in middle row, instead of pace based on desired finish time</string>
    
    <string id="lBlackBackground">Use black background instead of white</string>
    
	<string id="lPaceper300km">Pace averaged over the last .. seconds (max is 300 seconds)</string>
	<string id="lVertpace">Vertical speed averaged over the last 5 seconds</string>
	<string id="lrolavPacmaxsecs">Pace or/and speed averaged over the last ... seconds (max is 300 seconds)</string>
	<string id="lrolavPacmaxsecsError">This needs to be a number from 2 too 300</string>
	<string id="lPaceper100m">Pace in seconds per 100 meter (for track running)</string>

	<string id="lAverage3secSpeed">Speed averaged over the last 3 seconds in km/mile per hour</string>
	<string id="lAverage5secSpeed">Speed averaged over the last 5 seconds in km/mile per hour</string>
	<string id="lAverage30secSpeed">Speed averaged over the last .. seconds in km/mile per hour (max is 300 seconds)</string>

	<string id="lRCadence">Cadence in rotations per minute</string>
	<string id="lAEFactor">Average efficiency factor</string>
	<string id="lCEFactor">Current efficiency Index</string>
	<string id="lLEFactor">Current lap efficiency factor</string>

	<string id="lLLEFactor">Last lap average efficiency factor</string>

 </strings>

